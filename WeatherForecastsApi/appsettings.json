{
  "ConnectionStrings": {
    "WeatherForecastsDb": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=WeatherForecastsDb;Integrated Security=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Cors": {
    "Origins": [
      "http://example.com",
      "http://www.contoso.com"
    ],
    "Methods": [
      "OPTIONS",
      "GET",
      "POST",
      "PUT",
      "DELETE"
    ],
    "Headers": [
      "origin",
      "X-Request-Token",
      "Content-Type",
      "Accept",
      "Auth-Token",
      "Authorization"
    ]
  },
  "AllowedHosts": "*"
}
